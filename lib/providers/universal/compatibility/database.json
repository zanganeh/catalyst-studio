  "migrationGuidelines": {
    "general": [
      "Always validate data after transformation",
      "Test with sample content before full migration",
      "Document any custom transformations",
      "Plan for rollback scenarios"
    ],
    "richText": {
      "challenges": [
        "Format differences between platforms",
        "Custom blocks and marks",
        "Embedded content handling"
      ],
      "strategies": [
