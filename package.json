{
  "name": "catalyst-studio",
  "version": "0.1.0",
  "license": "CC-BY-NC-4.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "test:coverage:report": "jest --coverage && open coverage/lcov-report/index.html",
    "test:coverage:check": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "test:coverage:analyze": "node scripts/coverage-analysis.js",
    "test:coverage:full": "npm run test:coverage && npm run test:coverage:analyze",
    "test:benchmark": "node scripts/test-benchmark.js",
    "test:unit": "jest --testPathPattern='__tests__'",
    "test:integration": "jest --testPathPattern='integration'",
    "test:performance": "jest --detectOpenHandles --forceExit",
    "test:e2e": "playwright test",
    "test:e2e:parallel": "playwright test --workers=4",
    "test:e2e:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:report": "playwright show-report",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:fresh": "npm run db:reset -- --force --skip-seed && npm run db:seed",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:studio": "prisma studio",
    "test:db:setup": "dotenv -e .env.test -- prisma migrate deploy",
    "test:db:reset": "rm -f test.db test.db-journal && npm run test:db:setup",
    "migrate:content-items": "npx tsx scripts/migrate-content-items.ts",
    "migrate:content-items:rollback": "npx tsx scripts/migrate-content-items.ts rollback"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/react": "^1.2.12",
    "@hookform/resolvers": "^5.2.1",
    "@monaco-editor/react": "^4.6.0",
    "@openrouter/ai-sdk-provider": "^0.0.5",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.85.0",
    "@types/jszip": "^3.4.0",
    "ai": "^4.3.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "jszip": "^3.10.1",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
}
