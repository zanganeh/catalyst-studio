  "fallbackStrategies": {
    "simple": {
      "description": "Direct fallback to text/string type",
      "rules": [
        {
          "from": ["Email", "Phone", "URL"],
          "to": "Text",
          "addValidation": true
        }
      ]
    },
    "serialization": {
      "description": "Complex types serialized to string",
      "rules": [
        {
          "from": ["JSON", "MultiSelect"],
          "to": "Text",
          "transform": "serialize"
        }
      ]
    },
    "decomposition": {
      "description": "Break complex type into simpler fields",
      "rules": [
        {
          "from": "ContentArea",
          "to": "JSON",
          "transform": "decompose",
          "note": "Requires custom rendering logic"
        }
      ]
    },
    "approximation": {
      "description": "Use closest available type",
      "rules": [
        {
          "from": "Decimal",
          "to": "Number",
          "transform": "round",
          "warning": "Precision may be lost"
        }
      ]
    }
  },
  "transformationRules": {
    "universal_to_platform": {
      "steps": [
        "Check direct mapping exists",
        "If not, check fallback mapping",
        "Apply platform-specific attributes",
        "Add validation rules if needed",
        "Log transformation confidence"
      ]
    },
    "platform_to_universal": {
      "steps": [
        "Identify platform field type",
        "Map to universal type",
        "Preserve original metadata",
        "Extract validation rules",
        "Calculate confidence score"
      ]
    }
  },
  "confidenceFactors": {
    "directMapping": 100,
    "fallbackWithValidation": 85,
    "fallbackSerialization": 70,
    "fallbackDecomposition": 60,
